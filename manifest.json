{
  "manifest_version": 2,

  "name": "sine2imdb",
  "description": "Sinemalar.com film detay sayfasinda imdb linki olusturur.",
  "version": "1.0",

  "icons": {  "16": "icon16.png",
              "48": "icon48.png",
             "128": "icon128.png" },

  "content_scripts": [
    {
      "matches": ["http://www.sinemalar.com/film/*"],
      "js": ["jquery-2.1.4.min.js","script.js"]
    }
  ],

  "permissions": [
    "http://www.omdbapi.com/", "activeTab"
  ],

  "web_accessible_resources": ["imdb.png"]
}